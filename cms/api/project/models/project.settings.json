{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "name": "Project",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "date": {
      "type": "date"
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "sections.rich-text",
        "sections.large-media"
      ]
    },
    "coverImage": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "public": {
      "type": "boolean",
      "default": true,
      "unique": false,
      "required": true,
      "private": true
    }
  }
}
